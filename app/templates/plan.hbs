<ol class="breadcrumb">
  <li>{{#link-to 'refsets'}}Refsets{{/link-to}}</li>
  <li>{{#link-to 'refset.concepts'}}{{refset.title}}{{/link-to}}</li>
  <li class="active">Plan</li>
</ol>
<section id="plan">
  <h2>{{refset.title}}
    <div class="refset-actions">
      {{#link-to 'snapshots'}}<button class="first toggle btn btn-lg btn-info">Snapshots</button>{{/link-to}}
      {{#link-to 'plan.show' refset}}<button disabled="disabled" class="toggle btn btn-lg btn-info">Plan</button>{{/link-to}}
      {{#link-to 'refset.concepts' refset}}<button class="last toggle btn btn-lg btn-info">Concepts</button>{{/link-to}}
    </div>
  </h2>
  {{outlet}}
  {{render "select-concepts" modalid=selectConceptsModalId}}
</section>