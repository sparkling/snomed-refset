<section class="rules clearfix">  
  <h4 class="clearfix"><span class="title">Rules</span>
    <div class="plan-actions clearfix">
      {{#link-to 'plan.show'}}<button class="toggle btn edit-plan btn-sm btn-default" disabled="disabled">View</button>{{/link-to}}
      {{#link-to 'plan.edit'}}<button class="toggle btn edit-plan btn-sm btn-default">Edit</button>{{/link-to}}
      {{#link-to 'plan.concepts'}}<button class="last toggle btn btn-sm btn-default">Results</button>{{/link-to}}
    </div>
  </h4>
  <ul class="list-group list-rules">
    {{#each rule in model.rules}}
      <li class="list-group-item rule">
        {{render "showrule" rule}}
      </li>
    {{/each}}
  </ul>
</section>